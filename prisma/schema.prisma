datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Store {
  id String String @id @default(auto()) @map("_id")
  username  String 
  userId    String 
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
